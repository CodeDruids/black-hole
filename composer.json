{
    "name": "codedruids/black-hole",
    "description": "Testing class that ignores everything while still being valid",
    "keywords": ["black hole", "blackhole", "black-hole", "dummy", "ignore", "mock", "placeholder", "testing"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Leith Caldwell",
            "email": "leith@codedruids.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "php": ">=7.3.0",
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CodeDruids\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeDruids\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
